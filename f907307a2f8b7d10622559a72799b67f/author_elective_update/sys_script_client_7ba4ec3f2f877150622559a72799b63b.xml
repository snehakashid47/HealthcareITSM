        <name>Subcategories changes dynamically</name>
        <order/>
        <script><![CDATA[function onChange(control, oldValue, newValue, isLoading,isTemplate) {
    if (isLoading || newValue == '') {
        return;
    }

    // Get the category selected by the user
    g_form.getValue('category');

    // Set the subcategory based on the selected category
    if (newValue == 'IT_Equipment') {
        g_form.addOption('subcategory','hardware malfunction','Hardware Malfunction');
    } 
